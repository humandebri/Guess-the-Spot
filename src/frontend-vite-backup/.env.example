# Copy this file to .env.local for development
# Copy this file to .env.production for production

# Canister IDs (will be set during deployment)
VITE_INTEGRATED_CANISTER_ID=
VITE_GAME_ENGINE_CANISTER_ID=
VITE_PHOTO_NFT_CANISTER_ID=
VITE_REWARD_MINT_CANISTER_ID=

# Network configuration
VITE_HOST=http://localhost:4943
VITE_IDENTITY_PROVIDER=http://localhost:4943

# API Keys (required for production)
VITE_MAPBOX_API_KEY=your_mapbox_api_key_here

# Feature flags
VITE_ENABLE_MOCK_AUTH=false
VITE_ENABLE_DEV_TOOLS=false